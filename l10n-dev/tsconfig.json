{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"noEmitOnError": false,
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"inlineSources": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noUncheckedIndexedAccess": true
	},
	"exclude": [
		"node_modules",
		"dist",
		"**/*.test.ts"
	]
}
