{
	"name": "@vscode/l10n",
	"version": "0.0.3",
	"description": "A helper library to assist in localizing subprocesses spun up by VS Code extensions",
	"author": "Microsoft Corporation",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-l10n.git"
	},
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-l10n/issues"
	},
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"files": [
		"dist/*"
	],
	"devDependencies": {
		"@types/mocha": "^9.1.1",
		"@types/mock-fs": "^4.13.1",
		"@types/node": "^18.7.8",
		"@typescript-eslint/eslint-plugin": "^4.28.0",
		"@typescript-eslint/parser": "^4.28.0",
		"esbuild": "^0.15.5",
		"eslint": "^7.29.0",
		"import-fresh": "^3.3.0",
		"mocha": "^10.0.0",
		"mock-fs": "^5.1.4",
		"npm-dts": "^1.3.12",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1"
	},
	"scripts": {
		"compile": "rimraf dist && tsc -noEmit && node .esbuild.config.js",
		"lint": "eslint src --ext ts",
		"watch": "node .esbuild.config.js --watch",
		"test": "mocha",
		"prepublishOnly": "npm run lint && npm run compile && npm run test"
	}
}
